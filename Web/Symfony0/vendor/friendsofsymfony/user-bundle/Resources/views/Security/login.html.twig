{% trans_default_domain 'FOSUserBundle' %}


<html>
<body background="http://www.hdcarwallpapers.in/hdwallpapers/biker-and-bike-wallpapers.jpg">
	<link href="/bundles/vmotors/css/login.css" rel="stylesheet">
	<link href="/bundles/vmotors/css/bootstrap.min.css" rel="stylesheet">
	<div class="container">
    	<div class="row">
			<div class="col-md-6 col-md-offset-3">
				<div class="panel panel-login">
					<div class="panel-heading">
						<div class="row">
							<a href="/app_dev.php/login" class="active" id="login-form-link">Login</a>
						</div>
						<hr>
					</div>
					{% block fos_user_content %}
					{% if error %}
					<ul>
  					 <li>{{ error.messageKey|trans(error.messageData, 'security') }}</li>
  					 </ul>
					{% endif %}
					{% endblock fos_user_content %}
					<div class="panel-body">
						<div class="row">
							<div class="col-lg-12">
								<form id="login-form" action="{{ path("fos_user_security_check") }}" method="post" role="form" style="display: block;">
									<div class="form-group">
										<input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
										<input type="text" name="_username" id="username" tabindex="1" class="form-control" placeholder="Username" value="{{ last_username }}" required="required">
									</div>
									<div class="form-group">
										<input type="password" name="_password" id="password" tabindex="2" class="form-control" placeholder="Password" required="required">
									</div>
									<div class="form-group text-center">
										<input type="checkbox" tabindex="3" class="" name="remember" id="remember">
										<label for="remember"> Remember Me</label>
									</div>
									<div class="form-group">
										<div class="row">
											<div class="col-sm-6 col-sm-offset-3">
												<input type="submit" name="login-submit" id="login-submit" tabindex="4" class="form-control btn btn-login" value="Log In">
											</div>
										</div>
									</div>
									<div class="form-group">
										<div class="row">
											<div class="col-lg-12">
												<div class="text-center">
												</div>
											</div>
										</div>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
	<script src="/bundles/vmotors/js/bootstrap.min.js"></script>
	<script src="/bundles/vmotors/js/login.js" ></script>
</body>
	</html>